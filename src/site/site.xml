<?xml version="1.0" encoding="ISO-8859-1"?>
<project name="${project.name}">
    <bannerLeft>
        <name>SPF4J</name>
    </bannerLeft>
    <body>
        <links>
            <item name="SPF4J" href="www.spf4j.org"/>
        </links>

        <menu name="Main Menu">
            <item name="Introduction" href="index.html"/>
            <item name="Download" href="http://code.google.com/p/spf4j/downloads/list"/>
        </menu>

        <menu ref="modules" />
        <menu ref="reports" />
    </body>
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.3.0</version>
    </skin>
    <custom>
        <fluidoSkin>
            <topBarEnabled>true</topBarEnabled>
            <sideBarEnabled>false</sideBarEnabled>
            <topBarContainerStyle>width: 90%;</topBarContainerStyle>
            <googleSearch>
                <sitesearch/>
            </googleSearch>
            <facebookLike/>
            <googlePlusOne/>
        </fluidoSkin>
    </custom>
</project>